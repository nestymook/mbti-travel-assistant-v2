# Staging Environment Configuration
# MBTI Travel Planner Agent - HTTP Gateway Integration - Staging Settings

# Application Environment
ENVIRONMENT=staging
AWS_REGION=us-east-1

# Gateway HTTP Client Configuration
# GATEWAY_BASE_URL will use default from code: https://agentcore-gateway-mcp-tools-staging.bedrock-agentcore.us-east-1.amazonaws.com
GATEWAY_TIMEOUT=45
GATEWAY_MAX_RETRIES=3
GATEWAY_AUTH_REQUIRED=true
GATEWAY_AUTH_TOKEN=
GATEWAY_CONNECTION_POOL_SIZE=20
GATEWAY_KEEP_ALIVE_TIMEOUT=60

# Agent Model Configuration (Nova Pro)
AGENT_MODEL=amazon.nova-pro-v1:0
AGENT_TEMPERATURE=0.1
AGENT_MAX_TOKENS=2048
AGENT_TIMEOUT=60

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=./logs
ENABLE_FILE_LOGGING=true
ENABLE_JSON_LOGGING=true
ENABLE_STRUCTURED_LOGGING=true

# Health Check Configuration
HEALTH_CHECK_INTERVAL=300
ENABLE_BACKGROUND_HEALTH_CHECKS=true
GATEWAY_HEALTH_CHECK_ENDPOINT=/health

# Performance Configuration
HTTP_CLIENT_POOL_CONNECTIONS=20
HTTP_CLIENT_POOL_MAXSIZE=20
HTTP_CLIENT_MAX_KEEPALIVE_CONNECTIONS=10

# Monitoring Configuration
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_REQUEST_TRACING=true