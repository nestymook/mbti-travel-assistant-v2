# Development Environment Configuration
# MBTI Travel Planner Agent - HTTP Gateway Integration - Development Settings

# Application Environment
ENVIRONMENT=development
AWS_REGION=us-east-1

# Gateway HTTP Client Configuration
# GATEWAY_BASE_URL will use default from code: http://localhost:8080
GATEWAY_TIMEOUT=30
GATEWAY_MAX_RETRIES=2
GATEWAY_AUTH_REQUIRED=false
GATEWAY_AUTH_TOKEN=
GATEWAY_CONNECTION_POOL_SIZE=10
GATEWAY_KEEP_ALIVE_TIMEOUT=30

# Agent Model Configuration (Nova Pro)
AGENT_MODEL=amazon.nova-pro-v1:0
AGENT_TEMPERATURE=0.1
AGENT_MAX_TOKENS=2048
AGENT_TIMEOUT=60

# Logging Configuration
LOG_LEVEL=DEBUG
LOG_DIR=./logs
ENABLE_FILE_LOGGING=true
ENABLE_JSON_LOGGING=false
ENABLE_STRUCTURED_LOGGING=true

# Health Check Configuration
HEALTH_CHECK_INTERVAL=300
ENABLE_BACKGROUND_HEALTH_CHECKS=true
GATEWAY_HEALTH_CHECK_ENDPOINT=/health

# Performance Configuration
HTTP_CLIENT_POOL_CONNECTIONS=10
HTTP_CLIENT_POOL_MAXSIZE=10
HTTP_CLIENT_MAX_KEEPALIVE_CONNECTIONS=5
