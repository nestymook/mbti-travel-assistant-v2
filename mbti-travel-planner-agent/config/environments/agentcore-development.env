# AgentCore Development Environment Configuration

# Environment
ENVIRONMENT=development
DEBUG_MODE=true

# AgentCore Agent ARNs (Development)
RESTAURANT_SEARCH_AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:209803798463:runtime/restaurant_search_agent-mN8bgq2Y1j
RESTAURANT_REASONING_AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:209803798463:runtime/restaurant_search_result_reasoning_agent-MSns5D6SLE

# AgentCore Configuration
AGENTCORE_REGION=us-east-1
AGENTCORE_TIMEOUT=30
AGENTCORE_MAX_RETRIES=3

# Performance Configuration
PERFORMANCE_ENABLE_CACHING=true
PERFORMANCE_CACHE_TTL=300
PERFORMANCE_MAX_CONNECTIONS=50
PERFORMANCE_MAX_CONNECTIONS_PER_HOST=5
PERFORMANCE_ENABLE_PARALLEL=true

# Monitoring Configuration
MONITORING_ENABLE_METRICS=true
MONITORING_ENABLE_TRACING=true
MONITORING_ENABLE_HEALTH_CHECKS=true
MONITORING_HEALTH_CHECK_INTERVAL=60
MONITORING_METRICS_NAMESPACE=AgentCore/MBTI-Travel-Planner/Development

# AWS Configuration
AWS_REGION=us-east-1
AWS_DEFAULT_REGION=us-east-1

# Service Account Configuration (for machine-to-machine auth)
# SERVICE_ACCOUNT_PASSWORD=your-secure-password-here

# Development-specific settings
LOCAL_DEVELOPMENT=true
VERBOSE_LOGGING=true